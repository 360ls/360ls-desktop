{
  "name": "360ls-Desktop-Application",
  "productName": "360ls",
  "version": "1.0.3",
  "description": "The desktop application for displaying a local video live-stream and a local version of the DVR web application.",
  "keywords": [
    "live-stream",
    "python",
    "interface",
    "dvr"
  ],
  "author": "Luke Fernandez <lukejohnfernandez@gmail.com>",
  "contributors": [
    "Dong Yeop Lee, Spencer Lewis, Jonathan Witten"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/360ls/desktop/issues"
  },
  "homepage": "https://360ls.github.io/360ls/",
  "repository": {
    "type": "git",
    "url": "https://github.com/360ls/desktop.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "test/setup.js",
    "testPathDirs": [
      "test"
    ]
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty --ext .js --ext .jsx app/ test/",
    "dev": "npm run hot-server -- --start-hot",
    "test": "BABEL_DISABLE_CACHE=1 jest",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=1048 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "package": "npm run build && build --publish never",
    "package-linux": "npm run build && build --linux --armv7l",
    "cleanup": "mop -v",
    "release": "npm run package-linux"
  },
  "build": {
    "appId": "org.360ls.360ls",
    "asar": false,
    "category": "public.app-category.tools",
    "linux": {
      "target": [
        "deb",
        "zip"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.21",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "boiler-room-custodian": "^0.4.2",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.8",
    "electron-builder": "^8.6.0",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-flowtype-errors": "^1.5.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "html-webpack-plugin": "^2.24.0",
    "jest": "17.0.3",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "sinon": "^1.17.6",
    "spectron": "^3.4.0",
    "style-loader": "^0.13.1",
    "tcomb": "^3.2.15",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "cross-spawn": "^5.0.1",
    "dropbox": "^2.4.0",
    "electron-debug": "^1.0.1",
    "firebase": "^3.6.0",
    "font-awesome": "^4.7.0",
    "material-ui": "^0.16.4",
    "node-uuid": "^1.4.7",
    "normalizr": "^2.2.1",
    "react": "15.4.0",
    "react-dom": "^15.3.2",
    "react-player": "^0.12.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.6"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
